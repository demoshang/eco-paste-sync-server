{
  "name": "eco-paste-sync-server",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@9.7.0",
  "description": "",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "pnpm run clean && pnpm run build:bundle",
    "build:bundle": "tsup src/index.ts --treeshake --sourcemap --shims --format esm,cjs --dts",
    "clean": "rimraf dist",
    "dev": "nodemon --signal SIGHUP",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "start": "node --enable-source-maps --import @swc-node/register/esm-register ./src/index.ts"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.7",
    "axios": "^1.7.7",
    "dayjs": "^1.11.13",
    "hono": "^4.6.10",
    "radash": "^12.1.0",
    "ua-parser-js": "^2.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.9.1",
    "@swc-node/register": "^1.10.9",
    "@swc/cli": "0.5.0",
    "@swc/core": "1.4.11",
    "@types/node": "^22.9.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.14.0",
    "eslint-plugin-format": "^0.1.1",
    "nodemon": "^3.1.7",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  },
  "volta": {
    "node": "22.6.0",
    "pnpm": "9.7.0"
  }
}
